@model Vidly.ViewModels.RandomCustomersViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<table>
    <tr>
        <th>Customer</th>
    </tr>
    @foreach (var customer in Model.Customers)
    {
        <tr>
            <td><a href="">@customer.Name</a></td>
        </tr>
    }
</table>